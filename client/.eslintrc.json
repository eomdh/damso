{ 
  "env": { 
    "browser": true,
    "node": true,
    "es6": true
  }, 
  "extends": [ 
    "plugin:react/recommended", 
    "plugin:react-hooks/recommended", 
    "plugin:prettier/recommand" 
  ], 
  "parserOptions": {
    "ecmaVersion": 2020, 
    "ecmaFeatures": { 
      "jsx": true 
    }, 
    "sourceType": "module" 
  }, 
  "plugins": [ 
    "react", 
    "react-hooks", 
    "prettier", 
    "import" 
  ], 
  "rules": { 
    "arrow-body-style": "error", 
    "prefer-arrow-callback": "off", 
    "no-var": "error", 
    "no-dupe-keys": "error", 
    "react/prop-types": "off" 
  } 
}